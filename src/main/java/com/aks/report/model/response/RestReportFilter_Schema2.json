{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "reportName": {
      "type": "string"
    },
    "reportAttributes": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "fieldName": {
              "type": "string"
            },
            "fieldNameAlias": {
              "type": "string"
            },
            "columnWidth": {
              "type": "number"
            },
            "tableColumnName": {
              "type": "string"
            },
            "dateTimeFormat": {
              "type": "string"
            },
            "filter": {
              "type": "object",
              "properties": {
                "filterString": {
                  "type": "string"
                },
                "filterTypeId": {
                  "type": "integer"
                },
                "allowableValues": {
                  "type": "array",
                  "items": [
                    {
                      "type": "string"
                    }
                  ]
                }
              },
              "required": [
                "filterString",
                "filterTypeId",
                "allowableValues"
              ]
            },
            "reported": {
              "type": "boolean"
            }
          },
          "required": [
            "fieldName",
            "fieldNameAlias",
            "columnWidth",
            "tableColumnName",
            "dateTimeFormat",
            "filter",
            "reported"
          ]
        }
      ]
    }
  },
  "required": [
    "reportName",
    "reportAttributes"
  ]
}
