{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "gridInfo": {
      "type": "object",
      "properties": {
        "gridName": {
          "type": "string"
        },
        "tabName": {
          "type": "string"
        },
        "pageClass": {
          "type": "string"
        },
        "systemDefault": {
          "type": "boolean"
        }
      },
      "required": [
        "gridName",
        "tabName",
        "pageClass",
        "systemDefault"
      ]
    },
    "prefs": {
      "type": "object",
      "patternProperties": {
        ".{1,}": {
          "type": "object",
          "properties": {
            "value": {
              "type": "string"
            },
            "resolvedValue": {
              "type": "string"
            },
            "valueMap": {
              "type": "string"
            },
            "text": {
              "type": "string"
            },
            "preference": {
              "type": "string"
            },
            "id": {
              "type": "string"
            },
            "system": {
              "type": "integer"
            },
            "page": {
              "type": "string"
            },
            "parentSystemPreferenceId": {
              "type": "string"
            },
            "preferenceType": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                },
                "behaviour": {
                  "type": "string"
                },
                "regularExpression": {
                  "type": "string"
                },
                "regularExpressionFailMessage": {
                  "type": "string"
                },
                "enumValues": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "name",
                "behaviour",
                "regularExpression",
                "regularExpressionFailMessage",
                "enumValues"
              ]
            },
            "flag": {
              "type": "integer"
            },
            "regularExpression": {
              "type": "string"
            },
            "regularExpressionFailMessage": {
              "type": "string"
            },
            "markedForCreation": {
              "type": "boolean"
            },
            "systemLevelOnly": {
              "type": "boolean"
            },
            "displayName": {
              "type": "string"
            }
          },
          "required": [
            "value",
            "resolvedValue",
            "valueMap",
            "text",
            "preference",
            "id",
            "system",
            "page",
            "parentSystemPreferenceId",
            "preferenceType",
            "flag",
            "regularExpression",
            "regularExpressionFailMessage",
            "markedForCreation",
            "systemLevelOnly",
            "displayName"
          ]
        }
      }
    },
    "maxValues": {
      "type": "object",
      "properties": {
        "maxColumnCount": {
          "type": "integer"
        },
        "maxRowCount": {
          "type": "integer"
        }
      },
      "required": [
        "maxColumnCount",
        "maxRowCount"
      ]
    }
  },
  "required": [
    "gridInfo",
    "prefs",
    "maxValues"
  ]
}
